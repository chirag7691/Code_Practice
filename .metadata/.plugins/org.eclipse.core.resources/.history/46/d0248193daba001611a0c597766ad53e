 *      Author: Chirag R. Patel
 *
 * Summation of primes:
 *
 * The sum of the primes below 10 is 2 + 3 + 5 + 7 = 17.
 *
 * Find the sum of all the primes below two million.(2000000)
 */

#include <stdio.h>
int main()
{
	int i,j;
	long long sum;

	for (i = 2; i < 2000000; ++i)
	{
		if(i==2)
			sum = sum + i;

		for (j = 2; j < i; ++j)
		{
			if(i%j == 0)
				break;
		}
		if(i==j)
		{
			printf("%d\t",i);
			sum= sum + i;
		}
	}
	printf("sum of all the primes below two million.(2000000)= %lld",sum);
}

