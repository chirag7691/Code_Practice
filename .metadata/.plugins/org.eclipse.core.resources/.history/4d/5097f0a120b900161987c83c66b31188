#include<stdio.h>
int SumDivisibleBy (int n, int y);
int main()
{
	int i,j=3,k=5,sum=0;
	for(i=1;i<1000;i++)
	{
		if((SumDivisibleBy(i,j)==0 || SumDivisibleBy(i,k)==0))
			sum = sum + i;
	}
	printf("Total Sum = %d\n", sum);
}

int SumDivisibleBy (int n,int y)
{
	int res=0;
	res=n%y;
	return(res);
}
