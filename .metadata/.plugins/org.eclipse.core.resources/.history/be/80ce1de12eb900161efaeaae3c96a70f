/*
 * Problem_7:
 *
 * 10001st prime:
 *
 * By listing the first six prime numbers: 2, 3, 5, 7, 11, and 13, we can see that the 6th prime is 13.
 *
 * What is the 10001st prime number?
 */

#include<stdio.h>
int main()
{
	int i=3,n,count,c;

	printf("Enter the Number of prime number required\n");
	scanf("%d",&n);

	if ( n >= 1 )
 	{
		printf("First %d prime numbers are :\n",n);
		printf("1) 2\n");
	}

	for ( count = 2 ; count <= n ;  )
	{
		for ( c = 2 ; c <= i - 1 ; c++ )
		{
			if ( i%c == 0 )
            		break;
      		}
      		if ( c == i )
      		{
        		printf("%d) %d\n",count,i);
         		count++;
      		}
      		i++;
   	}
	/*
	* Problem_7_1:
	*
		int i,j,k,n,a;

		printf("Please Enter the number for number of prime number you want...\n");
		scanf("%d",&n);

		for(i=2;i<=n;i++)
		{

			if(n <= 1)
			printf("List of %d number of Prime numbers:\n 1) 2",n);

			for(a=2;a<i-1;a++)
			{
				if(n%a == 0)
					break;
				if(a == n)
					printf("%d) %d\n",i,a);
			}
		}
	*/
	return 0;
}
