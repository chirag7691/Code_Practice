#include<stdio.h>
void main()
{
	unsigned int i=0,j=0,k=1,l=0,sum=0;

	for(i=0;i>=0;i++)
	{
		if(i == 0)
        	{
      		     printf("%d, ", j);
       		     continue;
       		}
		
		l=j+k;
		j=k;
		k=l;
		printf("%d, ", l);

		if(l%2 == 0)
		{
			sum=sum+l;
			if(sum>4000000)
				break;
		}
	}
	printf("\nSum of Even fibonaci nymber= %d\n",sum);
}
