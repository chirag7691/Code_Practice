#include<stdio.h>
int main()
{
	int i,j,k,n,a;

	printf("Please Enter the number for number of prime number you want...\n");
	scanf("%d",&n);

	for(i=2;i<=n;i++)
	{
		
		if(n <= 1)
		printf("List of %d number of Prime numbers:\n 1) 2",n);

		for(a=2;a<i-1;a++)
		{
			if(n%a == 0)
				break;
			if(a == n)
				printf("%d) %d\n",i,a);
		}
	}
	return 0;
}
